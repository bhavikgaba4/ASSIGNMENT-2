// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int arr[5] = {2,1,4,1,5};
    int n = 5;
    
    for(int i = 0;i<4;i++){
        for(int j = 0;j<4;j++){
            if(arr[j+1]>arr[j]){
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
    for(int i = 0;i<5;i++){
        cout<<arr[i];
    }
    return 0;
}
