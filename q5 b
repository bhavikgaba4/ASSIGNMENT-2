// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int max = 4;
    int size = (3*max-2);
   int k = 0;
 int arr[size];
   for(int i = 0;i<size;i++){
       cin>>arr[i];
   }
   for(int i = 0 ;i<max;i++){
       for(int j = 0;j<max;j++){
           if(){
               cout<<arr[k];
               k++;
           }
           else{
               cout<<0;
           }
       }
       cout<<endl;
   }

    return 0;
}
